# Chave da API do Google Gemini
# Obtenha em: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=sua_chave_gemini_aqui

# Chave pública do Stripe (começa com pk_test_ ou pk_live_)
# Obtenha em: https://dashboard.stripe.com/apikeys
VITE_STRIPE_PUBLIC_KEY=pk_test_sua_chave_publica_aqui

# Chave secreta do Stripe (começa com sk_test_ ou sk_live_)
# ATENÇÃO: Nunca exponha esta chave no frontend em produção!
# Esta chave deve ser usada apenas no backend. No frontend, use apenas para testes.
STRIPE_SECRET_KEY=sk_test_sua_chave_secreta_aqui
